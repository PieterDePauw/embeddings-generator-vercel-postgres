{
	"name": "vercel-embeddings-generator",
	"version": "0.0.4",
	"private": true,
	"description": "A GitHub Action that converts your markdown files into embeddings and stores them in your Vercel Postgres database, allowing you to perform vector similarity search inside your documentation and website.",
	"main": "src/main.ts",
	"scripts": {
		"format": "prettier --write '**/*.ts'",
		"format-check": "prettier --check '**/*.ts'",
		"lint": "eslint src/**/*.ts",
		"package": "ncc build --source-map --license licenses.txt",
		"test": "jest",
		"all": "npm run format && npm run package",
		"db:check": "drizzle-kit check",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:pull": "drizzle-kit pull",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"db:up": "drizzle-kit up",
		"db:drop": "drizzle-kit drop"
	},
	"keywords": [
		"actions",
		"node",
		"setup"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@actions/core": "^1.11.1",
		"@ai-sdk/openai": "^0.0.72",
		"@jest/globals": "^29.7.0",
		"@vercel/postgres": "^0.10.0",
		"ai": "^3.4.33",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.36.1",
		"github-slugger": "^2.0.0",
		"mdast-util-from-markdown": "^1.3.1",
		"mdast-util-mdx": "^2.0.1",
		"mdast-util-to-markdown": "^1.5.0",
		"mdast-util-to-string": "^3.2.0",
		"mdast": "^3.0.0",
		"micromark-extension-mdxjs": "^1.0.1",
		"openai": "^4.72.0",
		"pg": "^8.13.1",
		"unist-builder": "^3.0.1",
		"unist-util-filter": "^4.0.1",
		"uuid": "^11.0.3"
	},
	"devDependencies": {
		"@types/estree": "^1.0.6",
		"@neondatabase/serverless": "^0.10.3",
		"@types/node": "^22.9.0",
		"@types/pg": "^8.11.10",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/parser": "^8.14.0",
		"@vercel/ncc": "^0.38.2",
		"drizzle-kit": "^0.28.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-github": "^4.10.2",
		"eslint-plugin-jest": "^28.9.0",
		"jest": "^29.7.0",
		"js-yaml": "^4.1.0",
		"prettier": "^3.3.3",
		"ts-jest": "^29.2.5",
		"tsx": "^4.19.2",
		"typescript": "5.7.2"
	}
}
